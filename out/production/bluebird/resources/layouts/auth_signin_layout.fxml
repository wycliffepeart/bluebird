<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="700.0" prefWidth="1100.0" style="-fx-background-color: white;" VBox.vgrow="ALWAYS">
         <children>
         
                                     <VBox alignment="CENTER" prefHeight="200.0" prefWidth="283.0" spacing="50.0" HBox.hgrow="ALWAYS">
            <children>
            
                                            <!--  -->
                    <VBox alignment="CENTER" prefWidth="383.0" spacing="30.0" VBox.vgrow="ALWAYS">
               <children>
                        <Text fill="#0606ee" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign In To Bluebird">
                           <font>
                              <Font name="System Bold" size="27.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="50.0" />
                           </VBox.margin>
                        </Text>
                        <VBox fx:id="fxIdFieldContainer" spacing="5.0">
                           <children>
                     
                                                         <!--  -->
                                 <TextField fx:id="fxIdNumber" onKeyTyped="#onEnterIdNumber" promptText="ID Number" style="-fx-border-color: #eeeeee; -fx-background-color: #f5f5f5; -fx-background-radius: 5; -fx-border-radius: 5;">
                                     <padding>
                                         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                     </padding>
                                 </TextField>
                           </children>
                        </VBox>
                        <VBox fx:id="fxPasswordContainer" spacing="5.0">
                           <children>
                     
                                                         <!--  -->
                                 <PasswordField fx:id="fxPassword" onKeyTyped="#onEnterPassword" promptText="Password" style="-fx-background-color: #f5f5f5; -fx-border-color: #eeeeee; -fx-background-radius: 5; -fx-border-radius: 5;">
                                     <padding>
                                         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                     </padding>
                                 </PasswordField>
                           </children>
                        </VBox>
                        <Label alignment="CENTER" onMouseClicked="#onClickForgetPassword" prefHeight="23.0" prefWidth="145.0" text="Forget Your Password?" textFill="#5b5a5a" underline="true">
                           <font>
                              <Font name="Segoe UI" size="12.0" />
                           </font>
                        </Label>
                        <VBox fx:id="fxSubmitButtonContainer" alignment="CENTER">
                           <children>
                     
                                                         <!--  -->
                                 <Button fx:id="fxSubmitButton" mnemonicParsing="false" onMouseClicked="#onClickSignIn" prefHeight="41.0" prefWidth="564.0" style="-fx-background-color: white; -fx-border-radius: 50%; -fx-background-radius: 50%; -fx-border-color: blue;" text="SIGN IN" textFill="BLUE">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 </Button>
                           </children>
                        </VBox>
               </children>
            
                                            </VBox>
               
                                                       <!--  -->
                   <Label alignment="CENTER" prefHeight="23.0" prefWidth="145.0" text="Design by Wycliffe Peart" textFill="#5b5a5a">
                       <font>
                           <Font name="Segoe UI" size="12.0" />
                       </font>
                   </Label>
            </children>
               <padding>
                  <Insets bottom="50.0" left="100.0" right="100.0" top="50.0" />
               </padding>
             </VBox>
            <VBox fx:id="fxSubmitButtonContainer1" alignment="CENTER" prefHeight="700.0" prefWidth="450.0" spacing="50.0" style="-fx-background-color: blue;">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello, Friend!">
                     <font>
                        <Font name="System Bold" size="29.0" />
                     </font>
                  </Text>
                  <Text fill="WHITE" layoutX="143.0" layoutY="351.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your personal details and start your journey with us" textAlignment="CENTER" wrappingWidth="300.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Text>
                  <Button fx:id="fxSidebarButton" mnemonicParsing="false" onMouseClicked="#onClickSidebarButton" prefHeight="41.0" prefWidth="382.0" style="-fx-background-color: blue; -fx-border-color: #eee; -fx-border-radius: 50%; -fx-background-radius: 50%;" text="SIGN UP" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" top="10.0" />
                     </padding>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
